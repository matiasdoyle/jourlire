{
	"name": "sympho.me - Reader Module",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Track when and what you read online.",
	"browser_action": {
		"default_popup" : "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"storage",
		"webNavigation",
		"http://*/*"
	]
}